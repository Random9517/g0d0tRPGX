    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "However\u30fcbehind every door was always the exact same peculiar room."
    },
    {
        "type": "ACTOR",
        "info": [
            "0",
            "chr_t0077a",
            "CENTER",
            "CENTER",
            "IN",
            "linear",
