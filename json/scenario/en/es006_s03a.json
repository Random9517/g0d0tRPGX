        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Mari",
        "text": "Eh? Eeeeeehhh!?"
    },
    {
        "type": "GROUP"
    },
    {
        "type": "ACTOR",
        "info": [
            "1",
