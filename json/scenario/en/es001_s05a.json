        "info": [
            "0",
            "yu_t002a",
            "CENTER",
            "CENTER",
            "IN",
            "linear",
            "1000"
        ]
    },
    {
        "type": "/GROUP"
    },
    {
        "type": "NAME_PLATE",
        "name": "Yukikaze",
        "text": "Prepare yourselves!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "(You shouldn't be the one to say that...!)"
    },
