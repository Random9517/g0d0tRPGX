    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Major smashed right through the window and the shutters, and disappeared in an instant."
    },
    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
