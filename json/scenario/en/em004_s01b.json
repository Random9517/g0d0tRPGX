    {
        "type": "NAME_PLATE",
        "name": "Hebiko",
        "text": "Kyaaah!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Lena unleashed her Demon Sword: Sakura Blossom with a flash, and a whirlwind rose from the ground."
    },
    {
        "type": "GROUP"
    },
    {
        "type": "BG",
        "info": [
            "nc016a",
