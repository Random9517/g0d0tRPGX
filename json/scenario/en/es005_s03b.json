        "type": "/GROUP"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "*Roooaaar!*"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "Ugh!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Another attack. I stumbled as I lost my footing, as well as my hold on Saya."
    },
    {
        "type": "NAME_PLATE",
        "name": "Hestia",
        "text": "Gotcha! I'll turn ya into spit roast!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Another flame sparked to life in Hestia's modified arm."
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "(Damn it\u30fc)"
