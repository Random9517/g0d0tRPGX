    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Meanwhile, in the new Cabinet Office building\u30fc"
    },
    {
        "type": "BGM_PLAY",
        "info": [
            "bgm206",
            "1000"
        ]
    },
    {
        "type": "BG",
        "info": [
