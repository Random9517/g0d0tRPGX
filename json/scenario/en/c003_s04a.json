    {
        "type": "NAME_PLATE",
        "name": "Shikanosuke",
        "text": "Ueeehheh!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "Shikanosuke",
        "text": "Wh-What the hell is this place!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "As soon as Shikanosuke shouted, the scenery around him changed completely."
    },
    {
        "type": "NAME_PLATE",
        "name": "???",
        "text": "You're an interesting kid."
    },
    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Shikanosuke",
        "text": "Eh!!?"
