    {
        "type": "NAME_PLATE",
        "name": "???",
        "text": "Good evening."
    },
    {
        "type": "NAME_PLATE",
        "name": "Shikanosuke",
        "text": "Aieeeeeee, it's a ghoooooost!!!?"
