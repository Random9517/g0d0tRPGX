        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Gargoyle",
        "text": "HROOOOOAGH!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "Whoa!? Watch out!!"
    },
    {
        "type": "SE_PLAY",
        "info": [
            "se006"
        ]
    },
    {
        "type": "ACTOR_OUT",
        "info": [
            "2",
            "CENTER",
            "linear",
            "1000"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "I raised my ninjato just in time and repelled the demonic creature's attack."
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "The circle of the creatures around us was getting smaller and smaller, and we had no more time left to spare."
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "S-See!? This is not the time to be arguing!!! Please listen to what I say!"
    },
    {
        "type": "ACTOR",
        "info": [
            "0",
            "kr_t001f",
            "CENTER",
            "CENTER",
            "IN",
            "linear",
            "1000"
        ]
    },
    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Kirara",
        "text": "No!! Absolutely not!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "Why!? And for starters, why do you hate the very sight of me!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "Kirara",
        "text": "Th-That's... because I don't like what I don't like!!"
    },
    {
        "type": "ACTOR",
        "info": [
            "0",
            "kr_t003b",
            "CENTER",
            "CENTER",
            "NONE",
            "linear",
            "0"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Kirara",
        "text": "I can't tolerate listening to a man!!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "Ah!? Kirara-senpai\u2015"
    },
