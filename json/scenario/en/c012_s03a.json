            "LEFT",
            "LEFT",
            "IN",
            "linear",
            "1000"
        ]
    },
    {
        "type": "ACTOR",
        "info": [
            "2",
            "ts_m082a",
            "RIGHT",
            "RIGHT",
            "IN",
            "linear",
            "1000"
        ]
    },
    {
