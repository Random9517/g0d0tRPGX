    {
        "type": "NAME_PLATE",
        "name": "Murasaki",
        "text": "So you are Sago? The government escort is waiting for you on the surface. Will you come with me, please?"
    },
    {
        "type": "ACTOR",
        "info": [
            "4",
            "m_t003a",
            "CENTER",
            "LEFT",
            "NONE",
            "linear",
