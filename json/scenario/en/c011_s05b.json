    {
        "type": "NAME_PLATE",
        "name": "Murasaki",
        "text": "\u2015Well then, your mission is complete. You are free to return to Gosha."
    },
    {
        "type": "NAME_PLATE",
        "name": "Us",
        "text": "\"Eh?\" \"Here?\" \"We're dismissed?\""
    },
    {
        "type": "BGM_PLAY",
        "info": [
            "bgm203",
