    {
        "type": "NAME_PLATE",
        "name": "Masked Madam",
        "text": "Leave the Zombies to me, now go!"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "The Masked Madam smiled graciously and turned toward the Zombies that came rushing in, preparing herself for battle."
