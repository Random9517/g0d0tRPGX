    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "We could hear the scarecrows' laughter close in behind us as they bounced along."
    },
    {
        "type": "GROUP"
    },
    {
        "type": "ACTOR_OUT",
        "info": [
            "0",
