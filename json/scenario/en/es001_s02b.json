    {
        "type": "NAME_PLATE",
        "name": "Hebiko",
        "text": "Yukikaze-chan, you really are amazing."
    },
    {
        "type": "ACTOR",
        "info": [
            "0",
            "yu_t001a",
            "LEFT",
            "LEFT",
