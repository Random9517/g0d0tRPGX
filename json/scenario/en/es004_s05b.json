    {
        "type": "NAME_PLATE",
        "name": "Gaku Shonosuke",
        "text": "Master. The Fuma Saika plan seems to have failed."
    },
    {
        "type": "NAME_PLATE",
        "name": "Nisha Gaiza",
        "text": "I see."
    },
    {
        "type": "ACTOR",
