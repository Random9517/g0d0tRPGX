    {
        "type": "NAME_PLATE",
        "name": "Shinobi Bear",
        "text": "Roooooooaaaaaaaaaarrrrr!!"
    },
    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Shikanosuke",
        "text": "Haiiiiiiiieeeeeeee!!? Not good, Fuma!! That bear is super strooooooooooooong!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "I know! For now just run for it!"
    },
    {
        "type": "GROUP"
    },
    {
        "type": "ACTOR_OUT",
        "info": [
            "0",
            "CENTER",
