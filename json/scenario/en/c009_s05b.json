        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Shonosuke",
        "text": "What a tremendous amount..."
    },
    {
        "type": "ACTOR",
        "info": [
            "2",
            "ts_m063a",
            "CENTER",
