    {
        "type": "NAME_PLATE",
        "name": "Shonosuke",
        "text": "Forgive me!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "*Bam*\u30fc!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "Onigumo Saburo",
        "text": "Ugh!!?"
    },
    {
        "type": "GROUP"
    },
