    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
    },
    {
        "type": "/GROUP"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "KABOOOOOOOOOOOM!!!"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "There was a sudden roar\u30fc"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "A tremor shook the entire church and fierce flames reached up from underground."
    },
    {
        "type": "BGM_PLAY",
        "info": [
            "bgm314",
            "1000"
        ]
    },
    {
        "type": "ACTOR",
        "info": [
            "2",
            "si_t003a",
            "CENTER",
            "CENTER",
            "IN",
            "linear",
            "1000"
        ]
    },
    {
        "type": "EFFECT_QUAKE",
        "info": [
            "VERTICAL",
            "0.5",
            "500"
        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Witch",
        "text": "\u30fc!? What's happening!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "A sign of unease ran across even the witch's face."
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "And then\u30fc"
    },
    {
        "type": "GROUP"
    },
    {
        "type": "ACTOR",
        "info": [
            "2",
            "si_t003a",
            "CENTER",
            "CENTER",
            "OUT",
