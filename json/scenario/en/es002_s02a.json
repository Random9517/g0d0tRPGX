        ]
    },
    {
        "type": "NAME_PLATE",
        "name": "Sakura",
        "text": "Hmhmmm\u266a It's the ever-intimate Sakura-sensei and\u30fc"
    },
    {
        "type": "ACTOR",
        "info": [
            "4",
            "m_t006a",
            "LEFT",
            "LEFT",
