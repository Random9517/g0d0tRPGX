    {
        "type": "NAME_PLATE",
        "name": "Saya NEO",
        "text": "Hey, onii-chan, get out of the way for a bit."
    },
    {
        "type": "NAME_PLATE",
        "name": "Me",
        "text": "H-Hey!?"
    },
    {
        "type": "NAME_PLATE",
        "name": "",
        "text": "Saya slowly stepped towards the soldiers."
